{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/virtual-facility/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/virtual-facility/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/virtual-facility",
  "projects": {
    "virtual-facility": {
      "type": "application",
      "root": "apps/virtual-facility",
      "entryFile": "main",
      "sourceRoot": "apps/virtual-facility/src",
      "compilerOptions": {
        "tsConfigPath": "apps/virtual-facility/tsconfig.app.json"
      }
    },
    "workflow-services": {
      "type": "application",
      "root": "apps/workflow-services",
      "entryFile": "main",
      "sourceRoot": "apps/workflow-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/workflow-services/tsconfig.app.json"
      }
    }
  }
}